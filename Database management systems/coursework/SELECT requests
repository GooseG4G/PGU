# КОД Б.1
SELECT `id_client` , `paid`
FROM `ORDER`;

# КОД Б.2
SELECT `id_client` , `id_admin` , `messages`
FROM `DIALOG`
WHERE `completed` = false;

# КОД Б.3
SELECT DISTINCT `id_client` , `id_admin` , `messages`
FROM `DIALOG`
WHERE `completed` = false and `id_admin` = 2

