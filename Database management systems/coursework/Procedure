delimiter $$
CREATE PROCEDURE CreateNewClient(
    IN client_id INT,
    IN client_full_name VARCHAR(45),
    IN client_email VARCHAR(45),
    IN client_ip VARCHAR(15),
    IN client_gender VARCHAR(6)
)
BEGIN
    INSERT INTO CLIENT (id_client, full_name, email, ip, gender)
    VALUES (client_id, client_full_name, client_email, client_ip, client_gender);
END $$
delimiter ;


delimiter $$
CREATE PROCEDURE GetLatestOrder()
BEGIN
    SELECT id_software, id_client, date_time, paid, completed, price
    FROM `ORDER`
    ORDER BY date_time DESC
    LIMIT 1;
END $$
delimiter ;

CREATE PROCEDURE show_unfinished_dialogs()
BEGIN
    SELECT * 
    FROM `DIALOG`
    WHERE comleted=false
END;
